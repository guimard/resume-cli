{
  "name": "resume-cli",
  "version": "0.0.0-development",
  "description": "The JSON Resume command line interface",
  "main": "build/index.js",
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "files": [
    "build/*"
  ],
  "types": "./build/main.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "ts-node src/main.ts",
    "lint": "eslint --ignore-path .gitignore .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonresume/resume-cli.git"
  },
  "keywords": [
    "Resume",
    "CV"
  ],
  "license": "MIT",
  "bin": {
    "resume": "build/main.js"
  },
  "bugs": {
    "url": "https://github.com/jsonresume/resume-cli/issues"
  },
  "homepage": "https://github.com/jsonresume/resume-cli#readme",
  "dependencies": {
    "browser-sync": "^2.29.1",
    "chalk": "^5.2.0",
    "commander": "^10.0.1",
    "js-yaml": "^4.1.0",
    "jsonresume-theme-even": "^0.18.1",
    "mime-types": "^2.1.35",
    "object-path-immutable": "^4.1.2",
    "puppeteer": "^20.1.0",
    "quaff": "^5.0.0",
    "read": "^2.1.0",
    "readline": "^1.3.0",
    "resume-schema": "^1.0.0",
    "stream-to-string": "^1.2.1",
    "superagent": "^8.0.9",
    "yesno": "^0.4.0",
    "z-schema": "^6.0.1",
    "z-schema-errors": "^0.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/browser-sync": "^2.27.0",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^2.1.1",
    "@types/read": "^0.0.29",
    "@types/superagent": "^4.1.17",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "rollup": "^3.21.4",
    "rollup-plugin-hashbang": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0"
  }
}
